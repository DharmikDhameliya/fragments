{
  "name": "fragments",
  "private": true,
  "version": "0.0.1",
  "description": "Fragments back-end API",
  "author": "Dharmik Dhameliya",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DharmikDhameliya/fragments.git"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "FRAGMENTS_LOG_LEVEL=debug nodemon ./src/server.js --watch src",
    "debug": "FRAGMENTS_LOG_LEVEL=debug nodemon --inspect=0.0.0.0:9229 ./src/server.js --watch src",
    "test": "jest -c jest.config.js --runInBand --",
    "test:watch": "jest -c jest.config.js --runInBand --watch --",
    "coverage": "jest -c jest.config.js --runInBand --coverage",
    "lint": "eslint \"./src/**/*.js\" \"tests/**/*.js\""
  },
  "dependencies": {
    "compression": "^1.8.1",
    "content-type": "^1.0.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "http-auth": "^4.2.1",
    "http-auth-passport": "^1.0.7",
    "jwks-rsa": "^3.2.1",
    "nanoid": "^3.3.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^10.3.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "stoppable": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.0",
    "prettier": "3.7.4",
    "supertest": "^7.2.2"
  }
}
